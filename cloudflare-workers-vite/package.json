{
  "name": "cloudflare-workers",
  "type": "module",
  "scripts": {
    "dev": "export NODE_ENV=development && vite",
    "build": "vite build",
    "preview": "export NODE_ENV=production && $npm_execpath run build && vite preview",
    "deploy": "export NODE_ENV=production && $npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "hono": "^4.9.4",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.7",
    "daisyui": "^5.0.50",
    "vite": "^7.1.3",
    "vite-ssr-components": "^0.5.0",
    "wrangler": "^4.32.0"
  }
}
